{
	"builds": [
		{
			"src": "apps/web/package.json",
			"use": "@vercel/next"
		},
		{
			"src": "packages/nestjs/dist/main.js",
			"use": "@vercel/node"
		}
	],
	"github": {
		"enabled": false
	},
	"public": false,
	"routes": [
		{
			"src": "^/api(?:/(.*))?$",
			"dest": "packages/nestjs/dist/main.js"
		},
		{
			"src": "/(.*)",
			"dest": "apps/web/$1"
		}
	],
	"version": 2
}
