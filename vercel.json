{
	"buildCommand": "turbo run website#build",
	"framework": "nextjs",
	"github": {
		"enabled": false
	},
	"installCommand": "bun install",
	"outputDirectory": "apps/web",
	"public": false,
	"regions": ["sin1"],
	"routes": [
		{
			"src": "^/api(?:/(.*))?$",
			"dest": "packages/nestjs/dist/main.js"
		},
		{
			"src": "/(.*)",
			"dest": "apps/web/$1"
		}
	],
	"version": 2
}
