{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": [".env"],
	"globalEnv": ["DATABASE_URL", "SUPABASE_URL", "SUPABASE_KEY"],
	"tasks": {
		"lint": {
			"cache": true,
			"persistent": true
		},
		"dev": {
			"cache": true,
			"persistent": true
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "dist/**", "!.next/cache/**"]
		}
	},
	"ui": "tui"
}
